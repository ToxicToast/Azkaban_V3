{
  "name": "admin-ui-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin-ui-e2e/src",
  "tags": [],
  "implicitDependencies": ["admin-ui"],
  "// targets": "to see all targets run: nx show project admin-ui-e2e --web",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/admin-ui-e2e/cypress.config.ts",
        "testingType": "e2e",
        "baseUrl": "http://localhost:3000"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/admin-ui-e2e/**/*.{js,ts}"]
      }
    }
  }
}
